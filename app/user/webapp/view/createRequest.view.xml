<mvc:View
    controllerName="app.user.controller.createRequest"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
    displayBlock="true"
    height="100%"
    busyIndicatorDelay="0"

    <OverflowToolbar id="_IDGenOverflowToolbar1" height="50px">
    <Title 
	  id="pageHeader"
	  text="Create Request"
	  textAlign="Center"
	></Title>
	<ToolbarSpacer id="_IDGenToolbarSpacer1" />
	<Button id="_IDGenButton1" text="Create" width="100px" press="onCreateRequest"></Button>
</OverflowToolbar>

<VBox id="_IDGenVBox" class="sapUiTinyMarginTop">
        <HBox id="_IDGenHBox1" class="formType">
            <Label id="_IDGenL" class="formTypeLabel" text="Form Type:" design="Bold"/>
            <ComboBox selectionChange="onValueHelpClose1" width="200px" items="{formTpyes>/}" selectedKey="{}" id="selectedDocumentType" change="">
                <core:Item key="{formTypes>}" text="{formTypes>}"/>
            </ComboBox>
        </HBox>
</VBox>

<Page id="_IDGenPage1" showHeader="false">
    <content>
      <Table id="createRequest">
      
      </Table>
	</content>
</Page>

